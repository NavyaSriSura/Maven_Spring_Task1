<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <!-- bean definitions here -->

    <bean id="actor" class="com.stackroute.domain.Actor">
        <constructor-arg name="name" index="0" value="Navya"/>
        <constructor-arg name="gender" index="1" value="female"/>
        <constructor-arg name="age" index="2" value="20"/>
    </bean>


    <bean id="movie1" autowire="constructor" class="com.stackroute.domain.Movie"/>

    <bean name="BeanLifeCycleDemoBean" class="com.stackroute.Demo.BeanLifeCycleDemoBean"/>
</beans>